{
  "name": "automated-apis-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://JsteReubsSoftware.github.io/",
  "dependencies": {
    "@react-oauth/google": "^0.12.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^1.6.8",
    "canvas": "^2.11.2",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.19.2",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mongoose": "^8.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-oauth-google": "^0.9.0",
    "react-router-dom": "^6.23.0",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.7.5",
    "universal-cookie": "^7.1.4",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "env-cmd -f ../.env react-scripts start",
    "build": "env-cmd -f ../.env react-scripts build",
    "test": "env-cmd -f ../.env react-scripts test",
    "eject": "env-cmd -f ../.env react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "client": "set PORT=5000 && env-cmd -f ../.env react-scripts start",
    "server": "env-cmd -f ../.env nodemon -r dotenv/config ../server/server.js",
    "dev": "concurrently \"yarn run client\" \"yarn run server\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "gh-pages": "^6.1.1",
    "nodemon": "^3.1.0",
    "tailwindcss": "^3.4.3"
  }
}
